{
  "name": "Koi Extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Koi Extension",
  "icons": {
    "512": "logo512.png"
  },
  "browser_action": {
    "default_icon": "logo512.png"
  },
  "background": {
    "scripts": ["script.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["storage"]
}
